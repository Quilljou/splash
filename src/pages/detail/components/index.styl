.label,
.value
  display: block

.label
    text-transform: uppercase;
    font-size: 20px;
    color: #bbb;
    line-height: 24px;

.value
  font-size: 36px;
  line-height: 48px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;

.exif-item
  margin-bottom: 10px
  width: 50%
  flex: 1
  display: inline-block
